@{
    ViewBag.Title = "AdminAdded";
    Layout = "~/Views/Shared/_myLayoutPage_Admin.cshtml";
}
<div class="jumbotron" style="background-color:#f5fffa">
    <div style="height:40px;">

    </div>
    <center>
        <table>

            <tr>
                <td>
                    <h2>Admin Added Successfully</h2>
                </td>
            </tr>
            <tr>
                <td>
                    <h4>Admin ID is @ViewBag.id</h4>
                </td>
            </tr>
            <tr>
                <td>
                    <h4>Autogenerated password is: @ViewBag.password </h4>
                </td>
            </tr>
            <tr>
                <td>
                    <div style="height:40px;"></div>
                </td>
            </tr>
            <tr>
                <td colspan="3">
                    <center> @Html.ActionLink("Add another Admin", "AddAdmin", "Admin", new { @Class = "btn btn-primary" })</center>
                </td>
            </tr>
        </table>
    </center>
    <div style="height:40px;">

    </div>
</div>